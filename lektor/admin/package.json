{
  "name": "lektor",
  "version": "0.0.0",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "bootstrap": "~3.3.0",
    "chai": "^4.2.0",
    "css-loader": "^3.4.2",
    "event-source-polyfill": "^1.0.12",
    "file-loader": "^6.0.0",
    "font-awesome": "^4.3.0",
    "jquery": "^3.5.0",
    "jsdomify": "^3.1.1",
    "json-loader": "^0.5.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "prop-types": "^15.7.2",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "standard": "^14.3.3",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "lint": "standard",
    "fix-lint": "standard --fix",
    "test": "nyc mocha static/js/**/*.test.js",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov",
    "dev": "webpack --watch --config ./static/webpack.config.js --context ./static",
    "webpack": "webpack --config ./static/webpack.config.js --context ./static"
  },
  "targets": "defaults",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "static/gen/"
    ],
    "globals": [
      "$LEKTOR_CONFIG"
    ]
  },
  "author": "",
  "license": "ISC"
}
