  "scripts": {
    "test": "nyc mocha",
    "build": "node -r esbuild-register build.ts",
    "dev": "node -r esbuild-register build.ts --watch",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov"
  },
  "mocha": {
    "spec": [
      "js/**/*.test.ts",
      "js/**/*.test.tsx"
    ],
    "require": "esbuild-register"
  },
  "nyc": {
    "extension": [
      ".jsx",
      ".js",
      ".tsx",
      ".ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "require": "esbuild-register"
  }
